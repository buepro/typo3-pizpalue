                                    <numIndex index="4" type="array">
                                        <numIndex index="0">LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:below</numIndex>
                                        <numIndex index="1">below</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </imagePosition>
                </el>
