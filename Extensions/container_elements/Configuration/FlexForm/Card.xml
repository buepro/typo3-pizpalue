<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:text</sheetTitle>
                <type>array</type>
                <el>
                    <titleText>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:title</label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </titleText>
                    <titleLayout>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.titleLayout
                        </label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <default>h3</default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>H1</label>
                                    <value>h1</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>H2</label>
                                    <value>h2</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>H3</label>
                                    <value>h3</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>H4</label>
                                    <value>h4</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>H5</label>
                                    <value>h5</value>
                                </numIndex>
                                <numIndex index="5" type="array">
                                    <label>H6</label>
                                    <value>h6</value>
                                </numIndex>
                            </items>
                        </config>
                    </titleLayout>
                    <titleClass>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.titleClass
                        </label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <default></default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>
                                     valueer_elements/Resources/Private/Language/locallang.xlf:none
                                    </label>
                                    <value></value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>H1</label>
                                    <value>h1</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>H2</label>
                                    <value>h2</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>H3</label>
                                    <value>h3</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>H4</label>
                                    <value>h4</value>
                                </numIndex>
                                <numIndex index="5" type="array">
                                    <label>H5</label>
                                    <value>h5</value>
                                </numIndex>
                                <numIndex index="6" type="array">
                                    <label>H6</label>
                                    <value>h6</value>
                                </numIndex>
                            </items>
                        </config>
                    </titleClass>
                    <titlePosition>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.titlePosition
                        </label>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <default></default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:default
                                    </label>
                                    <value></value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:left
                                    </label>
                                    <value>left</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:center
                                    </label>
                                    <value>center</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:right
                                    </label>
                                    <value>right</value>
                                </numIndex>
                            </items>
                        </config>
                    </titlePosition>
                    <titleLink>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.titleLink
                        </label>
                        <config>
                            <type>input</type>
                            <renderType>inputLink</renderType>
                            <eval>trim</eval>
                        </config>
                    </titleLink>
                    <bodyText>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.bodyText</label>
                        <config>
                            <type>text</type>
                            <enableRichtext>true</enableRichtext>
                        </config>
                    </bodyText>
                    <borderType>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.borderType
                        </label>
                        <onChange>reload</onChange>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <default></default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:none
                                    </label>
                                    <value></value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:header
                                    </label>
                                    <value>header</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:footer
                                    </label>
                                    <value>footer</value>
                                </numIndex>
                            </items>
                        </config>
                    </borderType>
                    <borderText>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.borderText
                        </label>
                        <displayCond>FIELD:borderType:REQ:true</displayCond>
                        <config>
                            <type>text</type>
                            <enableRichtext>true</enableRichtext>
                        </config>
                    </borderText>
                </el>
            </ROOT>
        </sDEF>
        <imageSheet>
            <ROOT>
                <sheetTitle>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:image</sheetTitle>
                <type>array</type>
                <el>
                    <flexform_imageReference>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:image</label>
                        <config>
                            <type>inline</type>
                            <foreign_table>sys_file_reference</foreign_table>
                            <foreign_field>uid_foreign</foreign_field>
                            <foreign_sortby>sorting_foreign</foreign_sortby>
                            <foreign_table_field>tablenames</foreign_table_field>
                            <foreign_match_fields>
                                <fieldname>flexform_imageReference</fieldname>
                            </foreign_match_fields>
                            <foreign_label>uid_local</foreign_label>
                            <foreign_selector>uid_local</foreign_selector>
                            <maxitems>1</maxitems>
                            <overrideChildTca>
                                <columns>
                                    <uid_local>
                                        <config>
                                            <appearance>
                                                <elementBrowserType>file</elementBrowserType>
                                                <elementBrowserAllowed>gif,jpg,jpeg,bmp,png,pdf,svg,ai</elementBrowserAllowed>
                                            </appearance>
                                        </config>
                                    </uid_local>
                                </columns>
                                <types type="array">
                                    <numIndex index="0" type="array">
                                        <showitem>--palette--;;imageoverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <showitem>--palette--;;imageoverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <showitem>--palette--;;imageoverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <showitem>--palette--;;audioOverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <showitem>--palette--;;videoOverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <showitem>--palette--;;imageoverlayPalette, --palette--;;filePalette</showitem>
                                    </numIndex>
                                </types>
                            </overrideChildTca>
                            <filter type="array">
                                <numIndex index="0" type="array">
                                    <userFunc>TYPO3\CMS\Core\Resource\Filter\FileExtensionFilter->filterInlineChildren</userFunc>
                                    <parameters>
                                        <allowedFileExtension>gif,jpg,jpeg,bmp,png,pdf,svg,ai</allowedFileExtension>
                                    </parameters>
                                </numIndex>
                            </filter>
                            <appearance>
                                <useSortable>1</useSortable>
                                <headerThumbnail>
                                    <field>uid_local</field>
                                    <width>45</width>
                                    <height>45c</height>
                                </headerThumbnail>
                                <showPossibleLocalizationRecords>0</showPossibleLocalizationRecords>
                                <showRemovedLocalizationRecords>0</showRemovedLocalizationRecords>
                                <showSynchronizationLink>0</showSynchronizationLink>
                                <showAllLocalizationLink>0</showAllLocalizationLink>
                                <enabledControls>
                                    <info>1</info>
                                    <new>0</new>
                                    <dragdrop>1</dragdrop>
                                    <sort>0</sort>
                                    <hide>1</hide>
                                    <delete>1</delete>
                                    <localize>1</localize>
                                </enabledControls>
                            </appearance>
                        </config>
                    </flexform_imageReference>
                    <imageWidth>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:width</label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                            <size>5</size>
                        </config>
                    </imageWidth>
                    <imagePosition>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:position</label>
                        <onChange>reload</onChange>
                        <config>
                            <type>select</type>
                            <renderType>selectSingle</renderType>
                            <default>top</default>
                            <items type="array">
                                <numIndex index="0" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:top
                                    </label>
                                    <value>top</value>
                                </numIndex>
                                <numIndex index="1" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:bottom
                                    </label>
                                    <value>bottom</value>
                                </numIndex>
                                <numIndex index="2" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:left
                                    </label>
                                    <value>left</value>
                                </numIndex>
                                <numIndex index="3" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:right
                                    </label>
                                    <value>right</value>
                                </numIndex>
                                <numIndex index="4" type="array">
                                    <label>
                                        LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:below
                                    </label>
                                    <value>below</value>
                                </numIndex>
                            </items>
                        </config>
                    </imagePosition>
                </el>
            </ROOT>
        </imageSheet>
        <customSheet>
            <ROOT>
                <sheetTitle>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:custom</sheetTitle>
                <type>array</type>
                <el>
                    <cardWrapClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.wrapClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </cardWrapClasses>
                    <cardClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.classes</label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                            <valuePicker>
                                <mode>append</mode>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:primaryBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-primary</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:secondaryBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-secondary</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:complementaryBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-complementary</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:tertiaryBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-tertiary</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:quaternaryBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-quaternary</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:lightBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-light</numIndex>
                                    </numIndex>
                                    <numIndex index="6">
                                        <numIndex index="0">
                                            LLL:EXT:pizpalue/Resources/Private/Language/Backend.xlf:darkBackgroundColor
                                        </numIndex>
                                        <numIndex index="1"> pp-panel pp-panel-dark</numIndex>
                                    </numIndex>
                                </items>
                            </valuePicker>
                        </config>
                    </cardClasses>
                    <bodyClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.bodyClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </bodyClasses>
                    <borderClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.borderClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </borderClasses>
                    <titleClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.titleClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </titleClasses>
                    <imageWrapClasses>
                        <label>
                            LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.imageWrapClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </imageWrapClasses>
                    <imageClasses>
                        <label>LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.imageClasses
                        </label>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                        </config>
                    </imageClasses>
                    <textColumnClasses>
                        <label>
                            LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.textColumnClasses
                        </label>
                        <displayCond>
                            <or>
                                <value1>FIELD:imageSheet.imagePosition:=:left</value1>
                                <value2>FIELD:imageSheet.imagePosition:=:right</value2>
                            </or>
                        </displayCond>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                            <default>col-md-8</default>
                        </config>
                    </textColumnClasses>
                    <imageColumnClasses>
                        <label>
                            LLL:EXT:container_elements/Resources/Private/Language/locallang.xlf:card.imageColumnClasses
                        </label>
                        <displayCond>
                            <or>
                                <value1>FIELD:imageSheet.imagePosition:=:left</value1>
                                <value2>FIELD:imageSheet.imagePosition:=:right</value2>
                            </or>
                        </displayCond>
                        <config>
                            <type>input</type>
                            <eval>trim</eval>
                            <default>col-md-4</default>
                        </config>
                    </imageColumnClasses>
                </el>
            </ROOT>
        </customSheet>
    </sheets>
</T3DataStructure>
