#=========================================================#
config {

    # ---------------------------------------------- #
    #  Css
    # ---------------------------------------------- #
    concatenateCss = 0
    compressCss = 0

    # ---------------------------------------------- #
    #  Javascript
    # ---------------------------------------------- #
    compressJs = 0
    concatenateJs = 0

    # ---------------------------------------------- #
