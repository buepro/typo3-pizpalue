plugin.bootstrap_package.settings.scss {
    #---------------------------------------------------------#
    # Bootstrap
    #---------------------------------------------------------#
    # Bootstrap main branch version
    pp-bootstrap-version = 5
    # Enable presets in extension user_pizpalue. See `EXT:pizpalue/Resources/Public/Scss/_presets.scss`
    pp-enable-user-presets = false
    # Set to `composer-root` if pizpalue is used as a root package (e.g. for development)
    pp-install-dir =

    #---------------------------------------------------------#
    # Photoswipe
    #---------------------------------------------------------#
    pswp__assets-path = ../../../../bootstrap_package/Resources/Public/Images/PhotoSwipe/
}
